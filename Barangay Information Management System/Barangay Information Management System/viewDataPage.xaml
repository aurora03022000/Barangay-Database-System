                                            <TextBlock Text="New Member" VerticalAlignment="Center"></TextBlock>
                                        </DockPanel>
                                    </Button>

                                    <Button x:Name="addProduct_btn" Cursor="Hand" Visibility="Hidden" AutomationProperties.Name="addProduct_btn" HorizontalAlignment="Left" Margin="10,107,0,109" Width="140" Height="34" Click="AddProduct_btn_Click" >
                                        <DockPanel>
                                            <Image Source="img/Add Product Icon.png" Height="25" Width="31" Margin="0,0,5,0" />
                                            <TextBlock Text="Add Product" VerticalAlignment="Center"></TextBlock>
                                        </DockPanel>
                                    </Button>

                                    <Button x:Name="cancel_new_product_btn" Visibility="Hidden" HorizontalAlignment="Left" Cursor="Hand" AutomationProperties.Name="cancel_new_product_btn" BorderBrush="Red" Background="Red" Foreground="White" VerticalAlignment="Top" Margin="165,115,0,0" Height="20" Width="50" Click="Cancel_new_product_btn_Click" >
                                        <DockPanel>
                                            <TextBlock x:Name="cancelNewProductTextBlock" Text="Cancel"  FontWeight="Bold" VerticalAlignment="Center" Foreground="White" AutomationProperties.Name="cancelNewProductTextBlock" Height="18"></TextBlock>
                                        </DockPanel>
                                    </Button>

                                    <Label x:Name="productWarning" Foreground="Red" FontStyle="Italic" Visibility="Hidden"  Content="Only Household Head Can Edit This Section." VerticalAlignment="Top" HorizontalAlignment="Left" Margin="165,167,0,0" AutomationProperties.Name="purokWarning"></Label>
                                    <Label x:Name="productWarning1" Foreground="Red" FontStyle="Italic" Visibility="Hidden"  Content="Only Household Head Can Edit This Section." VerticalAlignment="Top" HorizontalAlignment="Left" Margin="165,110,0,0" AutomationProperties.Name="purokWarning"></Label>

                                    <Button x:Name="edit_data_product_btn" Visibility="Visible" HorizontalAlignment="Left" Cursor="Hand" AutomationProperties.Name="edit_data_product_btn" Background="#FF47C144" Foreground="White" Margin="10,163,0,0" Width="140" Height="34" VerticalAlignment="Top" Click="Edit_data_product_btn_Click" >
                                        <DockPanel>
                                            <Image x:Name="editImageProduct"  Source="img/Edit Icon.png" Height="18" Width="20" Margin="0,0,5,0" AutomationProperties.Name="editImageProduct" />
                                            <TextBlock x:Name="editProductTextBlock" Text="Edit Data"  FontWeight="Bold" VerticalAlignment="Center" AutomationProperties.Name="editProductTextBlock"></TextBlock>
                                        </DockPanel>
                                    </Button>

                                    <Button x:Name="update_data_product_btn" Visibility="Hidden" HorizontalAlignment="Left" Cursor="Hand" AutomationProperties.Name="update_data_product_btn" Background="#FF47C144"  VerticalAlignment="Top" Margin="10,163,0,0" Height="34" Width="140" Click="Update_data_product_btn_Click">
                                        <DockPanel>
                                            <Image x:Name="updateImageProduct"  Source="img/Update Icon.png" Height="20" Width="24" Margin="0,0,5,0" AutomationProperties.Name="updateImageProduct" />
                                            <TextBlock x:Name="updateProductTextBlock"  Text="Update Data"  VerticalAlignment="Center" AutomationProperties.Name="updateProductTextBlock" Foreground="White" FontWeight="Bold"></TextBlock>
                                        </DockPanel>
                                    </Button>
                                    <Button x:Name="cancel_product_btn" Visibility="Hidden" HorizontalAlignment="Left" Cursor="Hand" AutomationProperties.Name="cancel_product_btn" BorderBrush="Red" Background="Red" Foreground="White" VerticalAlignment="Top" Margin="165,171,0,0" Height="20" Width="50" Click="Cancel_product_btn_Click" >
                                        <DockPanel>
                                            <TextBlock x:Name="cancelProductTextBlock" Text="Cancel"  FontWeight="Bold" VerticalAlignment="Center" Foreground="White" AutomationProperties.Name="cancelProductTextBlock" Height="18"></TextBlock>
                                        </DockPanel>
                                    </Button>
                                </Grid>
                            </Grid>
                        </Grid>
                    </Grid>
                </ScrollViewer>
            </Grid>
        </Grid>
        <!-- ** -->
    </Grid>
</Window>

