                                <!-- Not editable Data Grid  -->
                                <DataGrid Grid.Row="1" CanUserResizeColumns="false" HeadersVisibility="Column" x:Name="member_dataGrid" BorderBrush="White" Padding="10" Background="White" HorizontalGridLinesBrush="White" VerticalGridLinesBrush="White" FontSize="15" FontFamily="Tahoma" IsReadOnly="True" CanUserAddRows="False" ItemsSource="{Binding Temporary_Members}" AutoGenerateColumns="False" AutomationProperties.Name="member_dataGrid" VerticalScrollBarVisibility="Hidden" HorizontalAlignment="Left" HorizontalScrollBarVisibility="Hidden" CanUserResizeRows="False" Margin="-6,0,0,0" VerticalAlignment="Top" Visibility="Visible">
                                    <DataGrid.ColumnHeaderStyle >
                                        <Style TargetType="DataGridColumnHeader" >
                                            <Setter Property="Background" Value="White"/>
                                            <Setter Property="FontFamily" Value="Tahoma"></Setter>
                                            <Setter Property="Padding" Value="0,0,0,5"></Setter>
                                            <Setter Property="FontWeight" Value="Bold"></Setter>
                                        </Style>
                                    </DataGrid.ColumnHeaderStyle>

